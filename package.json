{
  "name": "holdmybeer",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspace holdmybeer run build",
    "dev": "yarn workspace holdmybeer run dev",
    "bootstrap": "yarn workspace holdmybeer run dev bootstrap",
    "rewrite": "yarn workspace holdmybeer run dev rewrite",
    "test": "yarn workspaces run test",
    "test:watch": "yarn workspaces run test:watch",
    "typecheck": "yarn workspaces run typecheck"
  }
}
